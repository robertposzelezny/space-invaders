<Window x:Class="gra.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:gra"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="800" ResizeMode="NoResize">
    <Canvas x:Name="main_canvas" Height="570" Width="790" Background="Black" Focusable="True" KeyDown="keyDown" KeyUp="keyUp">
        <Rectangle Name="playerRectangle" Height="34" Canvas.Left="350" Stroke="Black" Canvas.Top="504" Width="58">
            <Rectangle.Fill>
                <ImageBrush ImageSource="images/player.png"/>
            </Rectangle.Fill>
        </Rectangle>

    </Canvas>
</Window>
